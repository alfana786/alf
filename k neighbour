from sklearn. model _ selection import train_test_split
Import pandas as pd

sample_dataframe_df=pd.read_csv('')
sample_dataframe_df. head ( 5 )
sample_dataframe_df[Species].value_counts()
x=sample_dataframe_df.drop(['id','species'],axis=1)
y=sample_dataframe_df['Species']
x. head(5)
y. head(5)
train_x,test_x,train_y,test_y=train_test_split(x,y,train_size=0.8,random_state=80)
knn=KNeighborsClassifier(n_neighbors=5)
knn.fit(train_x,train_y)
knn.score(test_x,test_y)
